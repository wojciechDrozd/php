<?php 



echo "I'm a fucking bulldog, I will not fail I have to succeed <br/>
		when I pursue something I do it doggedly";

$url = "http://www.wiggle.co.uk/";
$ch = curl_init($url);
curl_setopt($ch,CURLOPT_URL,$url);
curl_setopt($ch, CURLOPT_RETURNTRANSFER, -1);
$urlContent = curl_exec($ch);

$regex = '@"bem-menu__list-item bem-menu__list-item--mponents">[^<]+?<a\shref="([^"]+?)"\sclass"@';
preg_match_all($regex,$urlContent,$matches);


echo "<pre>",$matches,"</pre>";





?>