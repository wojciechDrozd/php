<?php


//a simple array example
$array = array(
		
		"foo" => "bar",
		"bar" => "foo",
);

var_dump($array);



//simple array, var_dump vs print_r

$arr = array(1,2,3,4,5);


echo '<br><br><b>var_dump($arr):</b><br>';
echo "<pre>" , var_dump($arr) , "</pre>";

echo '<b>print_r($arr):</b><br>';
echo "<pre>" , print_r($arr) , "</pre>";

?>