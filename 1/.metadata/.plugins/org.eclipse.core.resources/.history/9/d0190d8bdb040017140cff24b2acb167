<?php

$str = "KasieÅ„ka queen of zi darkness";
echo "Original string: $str";
echo "<br>";

echo addslashes($str);
echo "<br>";

echo addcslashes($str, 'Ksi');
echo "<br>";

echo addcslashes($str, 'A..z');
echo "<br>";

echo addcslashes($str, 'z..A');
echo "<br>";

echo addcslashes($str, "\0..\101");
echo "<br>";


?>