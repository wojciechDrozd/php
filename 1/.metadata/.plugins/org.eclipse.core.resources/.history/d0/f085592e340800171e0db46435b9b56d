<?php


//evaluation

echo $foo=1;
print '<br>';
var_dump($foo);
print '<br>';

echo $foo = '1';
print '<br>';
var_dump($foo);
print '<br>';

echo $foo *= 2;
print '<br>';
var_dump($foo);
print '<br>';

echo $foo=$foo*1.3;
print '<br>';
var_dump($foo);
print '<br>';

$foo = 5 * "10 crazy piggies";
echo $foo;
print '<br>';
var_dump($foo);
print '<br>';

echo PHP_VERSION;
?>





























