function scanProducts(){
	
	var url = "www.nike.com";
	var name = "Nike Free RN";
	var extra = "878767";
	
	$.post("ajax/scanner.php", {
		
		url: url,
		name: name,
		extra: extra,
		
	}, function (data, status) {
        showProducts();
    });
	
}

function showProducts(){
	
	
	$.get("ajax/show_products.php", {}, function (data, status) {
        $(".results").html(data);
    });
	
}