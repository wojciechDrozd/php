<?php

if(isset($_POST['boxesString']) ){
	
	
	$boxesString =  $_POST['boxesString'];
	$allEvents = explode("|",$boxesString);
	$events =array();
	
	foreach($allEvents as $event){
	$events[] = explode(":",$event);
	}
	
	foreach($events as $event){
		foreach($event as $e){
			echo $e," ";
		}
		
		echo "<br/>";
	}
	
	echo $events[0][0];
	
	
	
	
}
?>